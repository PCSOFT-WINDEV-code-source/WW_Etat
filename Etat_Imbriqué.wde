  procedures :
   -
     name : InitialiserGraphe
     procedure_id : 1898018873703155227
     type_code : 14
     code : |1-
      Procédure InitialiserGraphe()
      
      	nIndice 	est un entier = 1
      	sCondition	est une chaine = ""
      
      // Spécifier un espacement entre les catégories de l'histogramme
      grHistoEspacement(GrapheVente, 10)
      
      // Modifier le sens du graphe
      grOrientation(GrapheVente, grordonnée, gradroite, grenhaut)
      
      // Définir la graduation
      grGraduation(GrapheVente, 7000, grOrdonnée)
      
      // Lire le premier enregistrement
      HLitPremier(REQ_CATEGORIE)
      
      TANTQUE PAS HEnDehors()
      	// Ajouter les données au graphique
      	grAjouteDonnée(GrapheVente, 1, nIndice,REQ_CATEGORIE.totalttc)
      
      	grEtiquetteCatégorie(GrapheVente, nIndice, REQ_CATEGORIE.libprod + <§$0000§> + REQ_CATEGORIE.IDLigneCde)
      	grLibellé(GrapheVente, grAfficheValeur, vrai)
      	
      	// Lire l'enregistrement suivant
      	HLitSuivant(REQ_CATEGORIE)
      
      	// Incrémenter l'indice de parcours
      	nIndice++
      fin
     type : 458752
